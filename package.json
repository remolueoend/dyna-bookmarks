{
  "name": "dyna-bookmarks",
  "version": "0.0.2",
  "description": "This extensions allows you to save and open browser bookmarks directly to or from a dynalist document.",
  "main": "dist/main.js",
  "repository": "git@github.com:remolueoend/dyna-bookmarks.git",
  "author": "Remo Zumsteg <remolueoend@users.noreply.github.com>",
  "homepage": "https://github.com/remolueoend/dyna-bookmarks",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clear": "rm -rf ./dist && rm -rf ./web-ext-artifacts",
    "build": "webpack --optimize-minimize --mode production && web-ext lint && web-ext build",
    "build.watch": "webpack -w",
    "start": "web-ext run",
    "dev": "yarn build && run-p build.watch start",
    "lint": "tslint --project .",
    "lint.fix": "yarn lint --fix",
    "test": "jest",
    "storybook": "start-storybook -c .storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^4.1.4",
    "@types/firefox-webext-browser": "^64.0.0",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/recompose": "^0.30.2",
    "@types/redux-actions": "^2.3.1",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-react-require": "^3.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.1",
    "identity-obj-proxy": "^3.0.0",
    "inquirer-directory": "^2.1.0",
    "jest": "^23.6.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "plop": "^2.2.0",
    "prettier": "^1.15.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.1.0",
    "web-ext": "^2.9.3",
    "web-ext-webpack-plugin": "https://github.com/hiikezoe/web-ext-webpack-plugin.git",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-webextension-plugin": "^0.1.1"
  },
  "dependencies": {
    "antd": "^3.11.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-webext": "^1.1.2",
    "styled-components": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{json,md,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  }
}
